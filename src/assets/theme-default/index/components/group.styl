.no-students {
  padding: 60px 197px 40px;
  background: #fff;
  display: flex;
  flex-direction: column;
  align-items: center;
  box-shadow: 0px 4px 8px 4px rgba(182, 182, 182, 0.08);
  border-radius: 8px;

  .image {
    width: 285px;
    height: 240px;
    background: url('~@/assets/theme-default/images/group/no_classmate.png') 50% 50%/285px;
    background-size: 100% 100%;
  }

  p {
    color: rgba(178, 178, 178, 1);
    line-height: 28px;
    text-align: center;
    margin-bottom: 40px;
  }

  .booking {
    width: 300px;
    height: 44px;
    text-align: center;
    line-height: 44px;
    border: 2px solid rgba(55, 56, 76, 1);
    border-radius: 6px;
  }
}