.el-button {border-width: 2px;}
/*带切换tabs的弹窗*/
.m-dialog-with-tabs{
    .el-dialog__header {
        height: 100px;
        background: rgba(89, 94, 155, 1);
        line-height: 60px;
        box-sizing: border-box;
        .el-dialog__title {
            color: #fff;
        }
    }
    .el-tabs{
        margin: 0 -30px;
    }
    .el-dialog__body {
        padding-top: 0px!important;
        min-height: 290px;
        .el-tabs__item {
            background: #595e9b;
            color: rgba(255, 255, 255, 0.5);
            font-size: 16px;
            font-family: Rubik-Regular, Rubik;
            font-weight: 400;
            white-space: pre-wrap;
            padding: 0 20px;
            width: 100px;
            word-break: break-word;
            line-height: 19px;
            height: 45px;
            &.is-active {
                color: #fff;
            }
        }
        .el-tabs__active-bar {
            background: #fff;
            margin-bottom: 2px;
            width: 120px;
        }
        .el-tab-pane {
            padding: 20px 30px;
        }
        .el-form-item {
            margin-bottom: 10px;
        }
    }
}
