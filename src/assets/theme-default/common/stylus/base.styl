@import '~@/assets/theme-default/common/stylus/main.styl';
.c-custom-radio{
    display inline-block
    position relative
    input{
        display block
        position absolute
        z-index 3
        top 0
        left 0
        opacity 0
        width 24px
        height 24px
    }
    label{
        padding-left 44px
        line-height 24px
    }
    label:after{
        content ''
        position absolute
        width 24px
        height 24px
        top 50%
        margin-top -12px
        left 0
        border 2px solid #d8d8d8
        background #fff
        border-radius 50%
        box-sizing border-box
        z-index 1
    }
    input:checked + label:after{
        border 2px solid #6f719f
    }
    label:before{
        content ''
        position absolute
        width 12px
        height 12px
        top 50%
        margin-top -6px
        left 6px
        background #6f719f
        border-radius 50%
        box-sizing border-box
        z-index 2
        display none
    }
    input:checked + label:before{
        display block
    }
}
.c-tab-title {color: #d78f25;line-height: 36px;}
